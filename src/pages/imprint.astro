---
import Layout from '../layouts/Layout.astro';
import * as content from '../../content/imprint.md';

const { title } = content.frontmatter;
---

<Layout title={title}>
  <main>
    <div class='max-w-4xl mx-auto my-20'>
      <div class='rich-text m-4'>
        <Fragment set:html={content.compiledContent()} />
      </div>
    </div>
  </main>
</Layout>
