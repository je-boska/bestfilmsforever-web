---
import Layout from '../layouts/Layout.astro';
import * as content from './index.md';

const { title, lead } = content.frontmatter;
---

<Layout title={title}>
  <main>
    <h1 class='text-2xl'>
      {title}
    </h1>
    <p>{lead}</p>
    <div set:html={content.rawContent} />
  </main>
</Layout>
